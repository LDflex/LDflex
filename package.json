{
  "name": "ldflex",
  "version": "2.12.0",
  "description": "A JavaScript DSL for querying Linked Data on the Web",
  "license": "MIT",
  "author": "Ruben Verborgh <ruben@verborgh.org> (https://ruben.verborgh.org/)",
  "contributors": [
    "Ruben Taelman <ruben.taelman@ugent.be> (https://www.rubensworks.net/)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LDflex/LDflex"
  },
  "main": "lib/index.js",
  "module": "module/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "lib",
    "module",
    ".babelrc"
  ],
  "dependencies": {
    "@rdfjs/data-model": "^1.1.2",
    "jsonld-context-parser": "^2.0.0",
    "sparqlalgebrajs": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@comunica/actor-init-sparql-file": "^1.21.3",
    "@ldflex/comunica": "^3.4.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "n3": "^1.10.0",
    "semantic-release": "^17.4.2"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:module",
    "build:lib": "babel src --out-dir lib",
    "build:module": "babel src --env-name module --out-dir module",
    "jest": "jest",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run jest -- --collectCoverage",
    "test:dev": "npm run jest -- --watch",
    "prepare": "husky install"
  },
  "release": {
    "branches": [
      "master",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
